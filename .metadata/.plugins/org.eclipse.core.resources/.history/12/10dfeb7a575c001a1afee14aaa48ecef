package D5;

import java.util.LinkedList;
import java.util.List;
import java.util.Scanner;

public class SE_D5_4534_트리흑백색칠 {
	static final int MOD = 1000000007;
	static int N;
	static List<Integer>[] adj;
	static long[][] memo; //색상, 정점번호  => 해당 번호를 해당 색상으로 칠할때 경우의 수
	
	
	public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
		
		int TC = sc.nextInt();
		
		for (int tc = 1; tc <= TC; tc++) {
			N = sc.nextInt();
			adj = new LinkedList<>[N+1];
			memo = new int[1 << (N+1)][N+1];
			visited= new boolean[N+1];
			
			for (int i = 0; i < M; i++) {
				int a = sc.nextInt();
				int b = sc.nextInt();
				adj[a][b] = 1;
				adj[b][a] = 1;
			}
			ans = 0;
			for (int i = 1; i <= N; i++) {
				ans = Math.max(ans, dfs(i, 1<<i));
//				visited[i] = true;
//				dfs(i,1);
//				visited[i] = false;
			}
			System.out.println("#"+tc+" "+ans);
		}
	}
	
	static long dfs(int v, int color, int present) {
		//memo[color][v] 값이 존재하면 다시 계산하지 않고 알고 있던거 리턴
		if(memo[color][v] != 0) 
			return memo[color][v];
		
		long ret = 1;
		//color가 흑이면 0, 백이면 1
		//color 흑이면
		if(color == 0) {
			
		}
		//자식 노드들을 백으로 칠한 경우의 경우의 수들 곱
		
		
		//color가 백이면
		else if(color == 1) {
			//자식 노드들을 흑으로 칠한 경우의 곱
			//자식 노드들을 백으로 칠한 경우의 곱
			//memo[color][v] 에 기록
			
			
		}
		return ret;
	}
}

package D5;

import java.util.Scanner;

public class SE_D4_8382_방향전환 {
	static class Point{
		int x, y, cnt;
		boolean dir;
		Point(int x, int y, int cnt, boolean dir){
			this.x = x;
			this.y = y;
			this.cnt = cnt;
			this.dir = dir;
		}
	}
	static int ans;
	static int sx;
	static int sy;
	static int fx;
	static int fy;
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int TC = sc.nextInt();
		
		for (int tc = 1; tc <= TC; tc++) {
			ans = 0;
			sx = sc.nextInt();
			sy = sc.nextInt();
			fx = sc.nextInt();
			fy = sc.nextInt();
			
			System.out.println("#"+tc+" "+ans);
		}
	}
}

			}
		}
        
        for (int i = 0; i < build_frame.length; i++) {
			for (int j = 0; j < 4; j++) {
				
			}
		}
        
        return answer;
    }
    
    static class Info{
    	boolean boEnd;
    	boolean boSrt;
    	boolean giEnd;
    	boolean giSrt;
    	
		public Info(boolean boEnd, boolean boSrt, boolean giEnd, boolean giSrt) {
			this.boEnd = boEnd;
			this.boSrt = boSrt;
			this.giEnd = giEnd;
			this.giSrt = giSrt;
		}
    }

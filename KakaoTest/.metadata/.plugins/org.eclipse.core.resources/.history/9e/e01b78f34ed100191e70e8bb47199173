import java.util.Scanner;

public class Test1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = "abcabcdede";
		int ans = solution(sc.nextLine());
		System.out.println(ans);
	}

	private static int solution(String s) {
		StringBuilder sb = new StringBuilder();
		for (int term = s.length() / 2; term >= 1; term--) {
			int repeat = 1;
			String ori = s.substring(0, term);
			for (int i = term; i <= s.length(); i += term) {
				if (i + term > s.length()) {
					if(repeat == 1)
						sb.append(s.substring(i-term, s.length()));
					else
						sb.append(repeat + ori);
				}else {
					String now = s.substring(i, i + term);
					if (now.equals(ori)) {
						repeat++;
					} else {
						if (repeat == 1) {
							sb.append(ori);
							ori = now;
						} else {
							sb.append(repeat + ori);
							ori = now;
							repeat = 1;
						}
					}
				}
			}
			if (s.length() > sb.length())
				break;
			else {
				if(term != 1)
					sb = new StringBuilder();
			}
		}
		

		System.out.println(sb.toString());
		return sb.length();
	}
}
